{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["prettier", "@typescript-eslint"],
  "ignorePatterns": "build",
  "rules": {
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
